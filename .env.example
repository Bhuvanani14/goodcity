# Environment Variables for Good City Application
# Copy this file to .env and fill in your actual values

# MongoDB Atlas Connection String
# Get this from MongoDB Atlas dashboard -> Clusters -> Connect -> Connect your application
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/good_city?retryWrites=true&w=majority

# JWT Secret Key
# Generate a secure random string for JWT token signing
JWT_SECRET=your-secure-jwt-secret-key-here-change-this-in-production

# Optional: Node Environment
NODE_ENV=development
